<%- messages() %>

<% if (title) { %>
    <h1><%= title %></h1>
    <% if (!errors) { %>
        <p>You're logged in</p>
    <% } %>
<% } else {
    res.redirect('/');
} %>

<% if (errors) { %>
    <ul class="notice">
        <% errors.array().forEach(error => { %>
            <li><%= error.msg %></li>
        <% }) %>
    </ul>
<% } %>


